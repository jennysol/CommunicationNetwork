<h1>Meus Inspetores</h1>
<p class="mb-4">Inspetores da sua equipe</p>
<div class="row">
  <% @following.each do |user| %>
    <div class="col-lg-4">
      <div class="card">
        <%= render("user_card_header", user: user) %>
        <div class="card-body">
          Email:<p class="card-text text-muted"><%= user.email %></p>
          Telefone:<p class="card-text text-muted"><%= user.phone %></p>
          Ind√∫stria:<p class="card-text text-muted"><%= user.industry %></p>
          <%= link_to("Deixar de Seguir", user_unfollow_path(user), method: :post, class: "col-12 btn btn-danger")%>
        </div>
      </div>
    </div> 
  <% end %>
</div>